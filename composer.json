{
    "name": "reecem/cashier-braintree",
    "description": "A Laravel Cashier fork.",
    "keywords": ["braintree", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        },
        {
            "name" : "Reece May",
            "email": "zsh.rce@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "braintree/braintree_php": "~6.0",
        "dompdf/dompdf": "^2.0",
        "illuminate/contracts": "~9.0",
        "illuminate/database": "~9.0",
        "illuminate/http": "~9.0",
        "illuminate/routing": "~9.0",
        "illuminate/support": "~9.0",
        "illuminate/view": "~9.0",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "~6.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "ReeceM\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReeceM\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "ReeceM\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
